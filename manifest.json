{
    "manifest_version": 3,
    "name": "WebEffects",
    "version": "1.0.1",
    "description": "A plain text description",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "sounds/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}